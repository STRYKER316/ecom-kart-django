    leader_only: true
  02_createsuperuser:
    command: "source /var/app/venv/*/bin/activate && python manage.py shell -c \"from django.contrib.auth import get_user_model; user = get_user_model(); user.objects.create_superuser('stryker', '316', 'stryker316', 'stryker316.games@gmail.com', 'djangoadmin123')\""
    leader_only: true
option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: ecom_kart.settings
